{
  "name": "whatsapp-web-media-player",
  "version": "0.0.1",
  "author": "Falko Krause",
  "description": "Play media files posted in conversations",
  "main": "index.js",
  "scripts": {
    "start": "concurrently  --kill-others 'npm run watch:build' 'npm run watch:sync'",
    "build": "./node_modules/.bin/webpack --config='./config/webpack.dev.js' --progress --profile --colors --display-error-details --display-cached",
    "watch:build": "npm run build -- --watch",
    "sync": "./node_modules/.bin/sync-files src/static dist/",
    "watch:sync": "./node_modules/.bin/sync-files --watch src/static dist/",
    "docu": "pwd && livingstyleguide compile ./src/sass/styleguide.lsg ./documentation/styleguide.html && nodejs node_modules/.bin/generate-md --layout github --input README.md --output ./documentation/",
    "zip": "npm run sync1; npm run build; cd dist/ ; zip -r ../whatsapp-web-media-player.chrome.app.zip . *",
    "icons": "./lib/convert-icomoon.js ./src/icons/media-player.zip ./src/sass/_icon.map.sass ./src/fonts/"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "redux": "^3.6.0",
    "vue": "^2.0.3",
    "vue-redux": "0.0.2"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "markdown-styles": "^3.1.8",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "sync-files": "^1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
