{
  "name": "audius",
  "version": "0.0.1",
  "author": "Falko Krause",
  "description": "Play media files posted in conversations",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:build",
    "test": "eslint ./src/**/*.js",
    "build:prod:app": "./node_modules/.bin/webpack --config='./config/webpack.production.app.js' --progress --profile --colors --display-error-details --display-cached",
    "build:prod:extension": "./node_modules/.bin/webpack --config='./config/webpack.production.extension.js' --progress --profile --colors --display-error-details --display-cached",
    "build": "./node_modules/.bin/webpack --config='./config/webpack.dev.js' --progress --profile --colors --display-error-details --display-cached",
    "watch:build": "npm run build -- --watch",
    "docu": "livingstyleguide compile ./src/sass/styleguide.lsg ./documentation/styleguide.html && nodejs node_modules/.bin/generate-md --layout github --input README.md --output ./documentation/",
    "zip": "npm run build:prod:extension; cd dist-extension/; zip -r ../audius.extension.zip . *",
    "icons": "./config/convert-icomoon.js ./src/icons/media-player.zip ./src/sass/_icon.map.sass ./src/fonts/"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "redux": "^3.6.0",
    "sortablejs": "^1.4.2",
    "vue": "^2.1.4",
    "vue-redux": "0.0.2",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.9.0",
    "circular-dependency-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "markdown-styles": "^3.1.8",
    "node-sass": "^3.8.0",
    "postcss-loader": "^1.3.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "sw-precache-webpack-plugin": "^0.6.3",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.6",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
